{
  "author": "Stefan Maric <me@stefanmaric.com> (http://stefanmaric.com)",
  "description": "Simple Table of Contents generator for DOM and Markdown",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-minify": "^0.4.0",
    "babel-preset-env": "^1.6.1",
    "documentation": "^6.3.1",
    "echint": "^4.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "esm": "^3.0.24",
    "gh-pages": "^1.0.0",
    "jsdom": "^11.9.0",
    "rollup": "^0.58.2",
    "tap": "^11.1.4"
  },
  "echint": {
    "ignore": [
      "coverage/**",
      "dist/**",
      "_docs/**",
      ".nyc-output/**"
    ]
  },
  "homepage": "http://stefanmaric.com/simpletoc/",
  "keywords": [
    "table of contents",
    "table",
    "contents",
    "toc",
    "dom",
    "markdown",
    "md"
  ],
  "license": "MIT",
  "main": "dist/simpletoc.umd.js",
  "module": "dist/simpletoc.esm.js",
  "name": "simpletoc",
  "repository": {
    "type": "git",
    "url": "https://github.com/stefanmaric/simpletoc.git"
  },
  "scripts": {
    "build": "for f in src/*.js; do npm run build-umd -- $f && npm run build-esm -- $f; done",
    "build-esm": "f(){ BN=$(basename $1 .js); rollup -f es $1 | tee dist/$BN.esm.js | babel-minify > dist/$BN.esm.min.js; }; f",
    "build-umd": "f(){ BN=$(basename $1 .js); rollup -f umd -n simpletoc $1 | babel -f $1 | tee dist/$BN.umd.js | babel-minify > dist/$BN.umd.min.js; }; f",
    "docs": "documentation build --config .documentation.yml -f html -o _docs src/",
    "lint": "echint && eslint . && documentation lint src/",
    "publish-docs": "npm run docs && gh-pages --dist _docs",
    "test": "npm run lint && tap --cov --nyc-arg=-i --nyc-arg=esm 'test/**/*.js'"
  },
  "version": "0.6.0"
}
